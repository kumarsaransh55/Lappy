@model List<OrderVM>

<div class="container">
    <div class="row pt-3 pb-3">
        <div class="col-6">
            <h2 class="text-primary">
                User List
            </h2>
        </div>
    </div>
    
    <table id="tblData" class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>
                    Name
                </th>
                <th>
                    Email
                </th>
                <th>
                    Phone Number
                </th>
                <th>
                    Company
                </th>
                <th>
                    Role
                </th>
                <th>
                    Actions
                </th>
            </tr>
        </thead>

    </table>
</div>


@section Scripts {
    <script src="~/js/user.js"></script>
}